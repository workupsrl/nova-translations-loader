{
  "name": "workup/nova-translations-loader",
  "description": "This Laravel Nova package helps developers load translations into their packages (forked by optimistdigital).",
  "keywords": [
    "laravel",
    "nova",
    "translations",
    "workup"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": "^8.0|^9.0|^10.0",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Workup\\NovaTranslationsLoader\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [],
      "aliases": {}
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
