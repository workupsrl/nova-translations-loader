{
  "name": "workup/nova-translations-loader",
  "description": "This Laravel Nova package helps developers load translations into their packages (forked by optimistdigital).",
  "keywords": [
    "laravel",
    "nova",
    "translations",
    "workup"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^7.21 || ^8.0 || ^9.0",
    "workup/nova": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Workup\\NovaTranslationsLoader\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [],
      "aliases": {}
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
